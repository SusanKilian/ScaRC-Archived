&HEAD CHID='sine2d_tol-3_fine_4x1', TITLE='flow through a 2D-channel with sinus inflow' /

&MESH IJK=40, 1, 40, XB=-0.4, -0.2, -0.01, 0.01, -0.1, 0.1, MULT_ID='mesh' /
&MULT ID='mesh', DX=0.20, DY=0.02, DZ=0.20, I_UPPER=3, J_UPPER=0, K_UPPER=0 /

&TIME T_END=3.142 / 

&PRES VELOCITY_TOLERANCE = 1E-3, MAX_PRESSURE_ITERATIONS = 100 /

&DUMP DT_PL3D=1.E10, VELOCITY_ERROR_FILE = .TRUE. /

&MISC DNS = .TRUE., STRATIFICATION =.FALSE., NOISE = .FALSE., GVEC=0.0, 0.0, 0.0 /

&SPEC ID='AIR', VISCOSITY=0.0E, BACKGROUND=.TRUE. /

&RADI RADIATION = .FALSE./

&SURF ID= 'WALL', DEFAULT= .TRUE., FREE_SLIP = .TRUE./

&SURF ID='INFLOW',VEL=-1.0,RAMP_V='IN'/

&VENT XB=-0.4,-0.4, -0.01,0.01, -0.1,0.1, SURF_ID='INFLOW' /
&VENT XB= 0.4, 0.4, -0.01,0.01, -0.1,0.1, SURF_ID='OPEN'  /

&DEVC XB= -0.40,-0.39, -0.01,0.01, -0.10,0.10, QUANTITY='U-VELOCITY', STATISTICS="VOLUME MEAN", ID='UMEAN_LEFT' , TIME_AVERAGED=.FALSE./
&DEVC XB=  0.39, 0.40, -0.01,0.01, -0.10,0.10, QUANTITY='U-VELOCITY', STATISTICS="VOLUME MEAN", ID='UMEAN_RIGHT', TIME_AVERAGED=.FALSE./

&DEVC XYZ=-0.4,0.0,0.0, QUANTITY='U-VELOCITY', ID='ULEFT'/
&DEVC XYZ= 0.4,0.0,0.0, QUANTITY='U-VELOCITY', ID='URIGHT'/

&DEVC XYZ= 0.0,0.0,0.0, QUANTITY='MAXIMUM VELOCITY ERROR', ID='VEL_ERROR' /
&DEVC XYZ= 0.0,0.0,0.0, QUANTITY='PRESSURE ITERATIONS'   , ID='PRES_ITE'  /

&SLCF PBY=0.0, QUANTITY='PRESSURE' /
&SLCF PBY=0.0, QUANTITY='H' /
&SLCF PBY=0.0, QUANTITY='VELOCITY', VECTOR=.TRUE. /
&SLCF PBY=0.0, QUANTITY='U-VELOCITY' /
&SLCF PBY=0.0, QUANTITY='W-VELOCITY' /
	
