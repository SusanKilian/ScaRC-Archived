&HEAD CHID='test', TITLE='Example of a tunnel simulation' /
&TIME T_END=2.0 /
&MESH IJK=20,10,10, XB=0.0,8.0,-2.0,2.0,0.0,4.0, MULT_ID='mesh' /
&MULT ID='mesh', DX=8., I_UPPER=3 /
&PRES SOLVER                   = 'SCARC'
      SCARC_MATRIX             = 'COMPACT'
      SCARC_METHOD             = 'MULTIGRID'
      SCARC_MULTIGRID          = 'ALGEBRAIC'
      SCARC_MULTIGRID_ACCURACY = 1.0E-6
      SCARC_MULTIGRID_THETA    = 2.0E-2 
      SCARC_MULTIGRID_LEVEL    = 2
      SCARC_SMOOTH             = 'FFT'
      SCARC_SMOOTH_ITERATIONS  = 8
      SCARC_COARSENING         = 'AGGREGATED' /
&MISC GVEC=-1.70,0.0,-9.65 /
&DUMP VELOCITY_ERROR_FILE=.TRUE. /
&REAC FUEL='PROPANE', SOOT_YIELD=0.015 /
&SURF ID='fire', COLOR='RED', HRRPUA=2000.0 /
&VENT SURF_ID='fire', XB=12.,12.2,-1.0,1.0,0.0,0.0, COLOR='RED' / 
&VENT SURF_ID='OPEN', PBX=32.0 / 
&SLCF QUANTITY='TEMPERATURE', VECTOR=.TRUE., PBY=0. /
&SLCF QUANTITY='H', CELL_CENTERED=.TRUE., PBY=0. /
&DEVC XYZ=0.,0.,0., QUANTITY='CPU TIME', ID='cpu' /
&TAIL /
