&HEAD CHID='3d_v2',TITLE='ScaRC-CG for 3D-cube with obstruction on 8 meshes' /

&MESH IJK=27,27,27, XB=0.0,2.70,0.0,2.70,0.0,2.70 /

&MISC NOISE = .FALSE.
      DNS   = .TRUE. /

&PRES SOLVER                   = 'USCARC'
      SCARC_MATRIX             = 'COMPACT'
      SCARC_METHOD             = 'MULTIGRID'
      SCARC_MULTIGRID          = 'ALGEBRAIC'
      SCARC_MULTIGRID_ACCURACY = 1.0E-6
      SCARC_MULTIGRID_THETA    = 2.0E-2 
      SCARC_MULTIGRID_LEVEL    = 2
      SCARC_SMOOTH             = 'SSOR'
      SCARC_SMOOTH_ITERATIONS  = 8
      SCARC_COARSENING         = 'AGGREGATED' /

&SPEC ID='BACKGROUND'
      BACKGROUND=.TRUE./

&TIME T_END=0.01 /

&DUMP VELOCITY_ERROR_FILE=.TRUE. /

&OBST XB=0.6,2.1,0.6,2.1,0.6,2.1, COLOR='BLUE' /

&SURF ID='INFLOW1',VEL=-2.0, RAMP_V='IN1' /
&SURF ID='INFLOW2',VEL=-2.0, RAMP_V='IN2'/
&SURF ID='INFLOW3',VEL=-2.0, RAMP_V='IN3'/

&VENT MB='XMIN', SURF_ID='INFLOW1' /
&VENT MB='XMAX', SURF_ID='OPEN' /

&SLCF PBX=0.4,QUANTITY='VELOCITY',VECTOR=.TRUE. /
&SLCF PBX=0.4,QUANTITY='H' /
&SLCF PBY=0.4,QUANTITY='VELOCITY',VECTOR=.TRUE. /
&SLCF PBY=0.4,QUANTITY='H' /
&SLCF PBZ=0.4,QUANTITY='VELOCITY',VECTOR=.TRUE. /
&SLCF PBZ=0.4,QUANTITY='H' /

&DEVC XYZ=0.64,0.00,0.41, QUANTITY='U-VELOCITY',ID='u-vel'    /
&DEVC XYZ=0.64,0.00,0.41, QUANTITY='V-VELOCITY',ID='v-vel'    /
